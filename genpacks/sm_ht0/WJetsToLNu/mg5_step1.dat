set run_mode 0

import model sm-no_b_mass

define vl = ve vm vt
define vl~ = ve~ vm~ vt~
define ell+ = e+ mu+ ta+
define ell- = e- mu- ta-

generate p p > ell+ vl $$ t t~ h
add process p p > ell+ vl j $$ t t~ h
add process p p > ell+ vl j j $$ t t~ h

add process p p > ell- vl~ $$ t t~ h
add process p p > ell- vl~ j $$ t t~ h
add process p p > ell- vl~ j j $$ t t~ h

output proc
